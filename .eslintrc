{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": [0],
    "no-dupe-args": [2],
    "no-dupe-keys": [2],
    "no-duplicate-case": [2],
    "no-extra-semi": [2],
    "no-irregular-whitespace": [0],
    "no-undef": [2],
    "valid-typeof": [2],
    "curly": [0],
    "dot-notation": [1],
    "eqeqeq": [2],
    "semi": [2, "always"],
    "quotes": [1, "single"],
    "no-unused-vars": [1],
    "array-bracket-spacing": [2, "never"],
    "brace-style": [2],
    "comma-spacing": [2],
    "comma-style": [2],
    "one-var": [2, "never"],
    "no-shadow": 0,
    "arrow-body-style": 0,
    "react/jsx-max-props-per-line": [2, {"maximum": 5}]
  }
}
